<div class="container">
  <h1> Add new student </h1>

  <%= form_with model: @add, url: '/student/create' do |form| %>
  <div>
    <%= form.label :Name %>
    <%= form.text_field :name %>
  </div>
    <div>
      <%= form.label :Email %>
      <%= form.text_field :email %>
    </div>

    <div>
      <%= form.submit %>
    </div>
  <% end %>

</div>
